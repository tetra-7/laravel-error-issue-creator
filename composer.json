{
  "name": "tetra7/laravel-error-issue-creator",
  "description": "Automatically creates or updates a GitHub issue for every HTTP 500 error in production.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "DhippoVendor\\ErrorIssueCreator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DhippoVendor\\ErrorIssueCreator\\ErrorIssueServiceProvider"
      ]
    }
  }
}